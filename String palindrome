#include <iostream>
using namespace std;
string is_palindrome(string s){
    int x=s.size();
    for(int i=0;i<x/2;i++){
        if(s[i]==s[x-i-1]){
            return "yes";
        }
        else
            return "no";
    }
}
int main()
{
    string s;
    cin>>s;
    
    string p=is_palindrome(s);
    cout<<p;
   return 0;
}
